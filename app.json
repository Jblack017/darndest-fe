{
  "expo": {
    "name": "Darndest",
    "description": "An App to save the funny stuff you children say. Enjoy!",
    "slug": "Darndest-fe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Darndest_Logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
